:Doblezoom
if #Count > 0
     #Count = #Count - 1
     goto :Zoom
endif
goto :ZoomSteps

:ZoomSteps
if #ZoomSteps == 1
     #ZoomSteps = #ZoomSteps + 1
     goto :Harvest
elseif #ZoomSteps == 2
     #ZoomSteps = #ZoomSteps + 1
     goto :Grow
elseif #ZoomSteps == 3
     #ZoomSteps = #ZoomSteps + 1
     goto :Sell
endif
goto :end

