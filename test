:Loadingtime
#Times = #Times + 1
#Color = 0
#Color1 = 0
if #Times > 25
	toast no found farm
	goto :Openfarm2
endif
getColor #Color 264 18
if #Color == -2754049
	toast abo ibrahim
	sleep 1000
  
	goto :Set
elseif #Times == 10
	getColor #Color1 50 450
elseif #Color1 == -27164
	toast abo ibrahim
	sleep 3000
	goto :Set
elseif #Color1 == 1290260
	touchDown 0 30 445
	sleep 1
	touchUp 0
	touchDown 1 75 460
	sleep 1
	touchUp 1
endif
sleep 5000
goto :Loadingtime

